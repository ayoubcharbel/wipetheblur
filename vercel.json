{
  "version": 2,
  "functions": {
    "api/telegram-webhook.js": {
      "maxDuration": 30
    },
    "api/setup-webhook.js": {
      "maxDuration": 30
    },
    "api/webhook-status.js": {
      "maxDuration": 30
    },
    "api/bot-status.js": {
      "maxDuration": 30
    },
    "api/bot-admin.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}